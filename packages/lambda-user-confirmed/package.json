{
  "name": "@whire-be/lambda-user-confirmed",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "nodemon --exec 'run-s start:ts bundle' --watch src --ext '*'",
    "start:ts": "ttsc",
    "build": "run-s typecheck bundle",
    "bundle": "node esbuild.config.js",
    "clean:light": "rm -rf ./dist && rm -rf ./tsconfig.build.tsbuildinfo",
    "clean:full": "yarn run clean:light && rm -rf ./node_modules",
    "typecheck": "tsc --noEmit",
    "precommit": "yarn run typecheck"
  },
  "dependencies": {
    "@lifeomic/axios-fetch": "^2.0.0",
    "@types/sharp": "^0.28.5",
    "@whire-be/common": "^1.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "aws4-axios": "^2.3.1",
    "axios": "^0.21.1",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.4",
    "lodash": "^4.17.21",
    "s3-imgurl-upload": "^1.2.6",
    "sharp": "^0.28.3",
    "source-map-support": "^0.5.19",
    "src": "link:./src"
  },
  "devDependencies": {
    "@anatine/esbuild-decorators": "^0.2.17",
    "aws-sdk": "2.965.0",
    "aws-sdk-mock": "^5.1.0",
    "esbuild": "^0.12.19",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "typescript-transform-paths": "^2.0.0"
  },
  "resolutions": {
    "follow-redirects": "^1.11.0"
  }
}
