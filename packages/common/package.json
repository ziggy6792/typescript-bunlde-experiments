{
  "name": "@whire-be/common",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "nodemon --exec 'run-s start:ts bundle' --watch src --ext '*'",
    "start:ts": "ttsc",
    "build": "ttsc",
    "bundle": "node esbuild.config.js",
    "clean:light": "rm -rf ./dist && rm -rf ./tsconfig.build.tsbuildinfo",
    "clean:full": "yarn run clean:light && rm -rf ./node_modules",
    "typecheck": "tsc --noEmit",
    "precommit": "yarn run typecheck",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "json-beautify": "^1.1.1",
    "src": "link:./src"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "jest": "^27.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.0.5",
    "tsconfig-paths-jest": "^0.0.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5",
    "ts-node": "^10.4.0",
    "typescript-transform-paths": "^2.0.0"
  },
  "peerDependencies": {
    "aws-sdk": "2.965.0"
  }
}
